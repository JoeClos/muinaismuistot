{
  "name": "muinaismuistot",
  "version": "1.2.4",
  "description": "Tutki kartalla Museoviraston ylläpitämien kulttuuriperintörekistereiden paikkatietoa.",
  "scripts": {
    "start": "http-server ./dist -p 8091 -c-1 -a localhost -o",
    "build-prod": "webpack --mode=production --progress",
    "build-prod-and-start": "npm run build-prod && npm run start",
    "start-webpack-dev-server": "webpack-dev-server --mode=development --port 8091 --progress --hot --colors --inline",
    "build-dev": "webpack --mode=development --progress --devtool source-map",
    "build-dev-and-start": "npm run build-dev && npm run start",
    "test": "jest"
  },
  "author": "Antti Kekki",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/anttikekki/muinaismuistot.git"
  },
  "dependencies": {
    "@babel/polyfill": "^7.0.0",
    "bootstrap": "3.4.1",
    "jquery": "3.4.1",
    "ol": "5.3.3",
    "proj4": "2.5.0"
  },
  "devDependencies": {
    "babel-core": "6.26.3",
    "babel-jest": "23.6.0",
    "babel-loader": "7.1.5",
    "babel-preset-env": "1.7.0",
    "clean-webpack-plugin": "1.0.0",
    "copy-webpack-plugin": "4.6.0",
    "css-loader": "1.0.1",
    "file-loader": "2.0.0",
    "html-webpack-plugin": "3.2.0",
    "http-server": "0.11.1",
    "jest": "23.6.0",
    "prettier": "1.15.3",
    "style-loader": "0.23.1",
    "url-loader": "1.1.2",
    "webpack": "4.39.2",
    "webpack-cli": "3.3.7",
    "webpack-dev-server": "3.8.0"
  }
}
