{
  "name": "Muinaismuistot",
  "version": "1.1.0",
  "description": "Tutki kartalla Museoviraston ylläpitämien kulttuuriperintörekistereiden paikkatietoa.",
  "scripts": {
    "build-dist": "webpack --mode=production --progress --optimize-minimize",
    "build-dist-and-start": "npm run build-dist && http-server ./dist -p 8091 -c-1 -a localhost -o",
    "build-dev-amd-start-dev-server": "npm run build-dev && webpack-dev-server --mode=development --port 8091 --progress --hot --colors --inline --content-base dist",
    "build-dev": "webpack --mode=development --progress --devtool source-map --output-pathinfo"
  },
  "author": "Antti Kekki",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/anttikekki/muinaismuistot.git"
  },
  "devDependencies": {
    "@babel/core": "7.2.0",
    "babel-loader": "8.0.4",
    "clean-webpack-plugin": "1.0.0",
    "copy-webpack-plugin": "4.6.0",
    "css-loader": "1.0.1",
    "file-loader": "2.0.0",
    "html-webpack-plugin": "3.2.0",
    "http-server": "0.11.1",
    "style-loader": "0.23.1",
    "url-loader": "1.1.2",
    "webpack": "4.27.1",
    "webpack-cli": "3.1.2",
    "webpack-dev-server": "3.1.10"
  },
  "dependencies": {
    "bootstrap": "3.3.7",
    "jquery": "3.2.1",
    "ol": "4.4.2",
    "proj4": "2.4.4"
  }
}
