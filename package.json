{
  "name": "Muinaismuistot",
  "version": "1.0.0",
  "description": "Tutki kartalla Museoviraston ylläpitämien kulttuuriperintörekistereiden paikkatietoa.",
  "scripts": {
    "build-dist": "webpack --progress --optimize-minimize",
    "build-dist-and-start": "npm run build-dist && http-server ./dist -p 8091 -c-1 -a localhost -o",
    "start-dev-server": "npm run build-dev && webpack-dev-server --port 8091 --progress --hot --colors --inline --content-base dist",
    "build-dev": "webpack --progress --devtool source-map --output-pathinfo"
  },
  "author": "Antti Kekki",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "css-loader": "^0.28.4",
    "style-loader": "^0.18.2",
    "webpack": "^3.3.0",
    "webpack-dev-server": "^2.6.1",
    "http-server": "0.8.0",
    "file-loader": "^1.1.5",
    "html-webpack-plugin": "^2.30.1",
    "url-loader": "^0.6.2"
  },
  "dependencies": {
    "bootstrap": "^3.3.7",
    "jquery": "^3.2.1",
    "ol": "^4.2.2",
    "proj4": "^2.4.4"
  }
}
